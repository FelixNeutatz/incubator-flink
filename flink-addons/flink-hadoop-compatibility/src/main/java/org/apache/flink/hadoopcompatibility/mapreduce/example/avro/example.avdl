@namespace("org.apache.flink.hadoopcompatibility.mapreduce.example.avro.java")
protocol FlinkParquetAvro {

enum AminoAcidType {
	ALIPHATIC,
	HYDROXYL,
	CYCLIC,
	AROMATIC,
	BASIC,
	ACIDIC
}

record AminoAcid {
 	AminoAcidType type;
	string fullName;
	string abbreviation;
	float molecularWeight;
}

}
